{
  "name": "jsonnet-formatter",
  "displayName": "jsonnet Formatter",
  "description": "Provide jsonnet vscode format support.",
  "version": "0.2.0",
  "publisher": "xrc-inc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xreception/vscode-jsonnet-formatter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xreception/vscode-jsonnet-formatter/issues"
  },
  "homepage": "https://github.com/xreception/vscode-jsonnet-formatter#readme",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:jsonnet"
  ],
  "main": "./out/main",
  "contributes": {
    "languages": [
      {
        "id": "jsonnet",
        "aliases": [
          "Jsonnet"
        ],
        "extensions": [
          ".jsonnet"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf ./out; npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^8.0.51",
    "@types/diff": "^3.2.2",
    "typescript": "^2.6",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "diff": "^3.4.0"
  }
}